{
  "name": "Yurl",
  "version": "1.0",
  "description": "Stash to Product",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["page.js"]
    }
  ],
  "background": { "scripts": ["background.js"] },
  "browser_action": {
    "default_title": "Time to current page",
    "default_icon": "orange.png" // green
    
  },
  "permissions": [
	"http://*/*",
	"https://*/*",
	"tabs"
  ],
  "manifest_version": 2
}
